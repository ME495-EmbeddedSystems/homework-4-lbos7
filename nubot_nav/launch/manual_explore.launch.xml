<launch>
    <arg name="use_rviz" default="true" />
    <arg name="world"    default="nubot_simple.sdf" />
    <include file="$(find-pkg-share nubot)/launch/simulate.launch.xml">
        <arg name="use_rviz" value="$(var use_rviz)"/>
        <arg name="world" value="$(var world)"/>
    </include>
</launch>